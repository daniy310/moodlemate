<!-- set default course display for course format "topics" & "OneTopics"-->
<script>
var formatElement = document.querySelector('[name="format"]');

if (formatElement){
    var format = formatElement.value;
    if (format == "topics") {
        // find the coursedisplay
        var element = document.querySelector('[name="coursedisplay"]');
        // Loop through the options to find the one with value="0" and set it as selected
        for (var i = 0; i < element.options.length; i++) {
            if (element.options[i].value === "0") {
                element.options[i].selected = true;
                break; // Exit the loop once the option is found
            }
        }
    }
    else if (format == "onetopic") {
         // find the coursedisplay
         var element = document.querySelector('[name="coursedisplay"]');
         // Loop through the options to find the one with value="0" and set it as selected
         for (var i = 0; i < element.options.length; i++) {
             if (element.options[i].value === "1") {
                 element.options[i].selected = true;
                 break; // Exit the loop once the option is found
             }
         }

    }
}
</script>

<!-- Additional H5P resizer-->
<script>
(function() {
    if (!window.postMessage || !window.addEventListener || window.h5pResizerInitialized2)
        return;
    window.h5pResizerInitialized2 = true;
    var actionHandlers = {};

    var getHeaderHeight = (iframe) => {
        var header = iframe.contentWindow.document.querySelector('.activity-header');
        var style = window.getComputedStyle(header);
        var styleC = window.getComputedStyle(header.children[header.children.length - 1]);
        return header.scrollHeight + parseFloat(style.marginBottom) + parseFloat(styleC.marginBottom);
    };
    actionHandlers.prepareResize = function(iframe, data) {
        var h = getHeaderHeight(iframe);
        var sh = data.scrollHeight + h,
            ch = data.clientHeight + h;
        if ((iframe.clientHeight !== sh || data.scrollHeight !== ch) && iframe.style.height)
            iframe.style.minHeight = ch + 'px';
    };

    actionHandlers.resize = function(iframe, data) {
        if (iframe.style.height)
            iframe.style.minHeight = data.scrollHeight + getHeaderHeight(iframe) + 'px';
    };

    window.addEventListener('message', function receiveMessage(event) {
        if (event.data.context !== 'h5p' || !window.h5pResizerInitialized) return;
        var iframe = [...document.getElementsByTagName('iframe')].find(f => f.contentWindow == event.source);
        if (!iframe) return;
        if (actionHandlers[event.data.action])
            actionHandlers[event.data.action](iframe, event.data);
    }, false);

    //wait until we are sure the original script has loaded and then manually trigger a resize
    var initInt = setInterval(function(){
        if(window.h5pResizerInitialized){
            var tar = [...document.getElementsByTagName('iframe')]
                .filter(f => f.src.indexOf('h5p') !== -1 || f.src.indexOf('hvp') !== -1);
            tar.forEach(f => f.style.width = "99%");
            setTimeout(() => tar.forEach(f => f.style.width = "100%"), 100);
            clearInterval(initInt);
        }
    }, 100);
})();
</script>

