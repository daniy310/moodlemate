<!-- To hide quiz attempt button in Proctorio exams - Updated by AB on 10-05-2024 -->
<script>
window.addEventListener('load', () => {
    const checkAndHideElement = () => {
        const targetDiv = document.querySelector('.singlebuttonrui-quizstartbuttondiv');
        if (targetDiv) targetDiv.style.display = 'none';
    };

    if (document.body.id === "page-mod-quiz-view" && document.querySelector('.proctorio-iembed')) {
        checkAndHideElement();
        const observer = new MutationObserver(mutations => {
            mutations.forEach(mutation => {
                if (mutation.type === 'attributes' && mutation.attributeName === 'style') {
                    checkAndHideElement();
                }
            });
        });

        const targetDiv = document.querySelector('.singlebuttonrui-quizstartbuttondiv');
        if (targetDiv) {
            observer.observe(targetDiv, { attributes: true });
        }
    }
});

</script>

